{
  "name": "velox",
  "version": "1.0.0",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "prebuild": "expo prebuild",
    "android": "expo prebuild && expo run:android",
    "web": "expo start --web"
  },
  "dependencies": {
    "expo": "^49.0.0",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-native": "0.72.0",
    "typescript": "~5.2.2",
    "expo-file-system": "~15.3.0",
    "expo-secure-store": "~12.0.0",
    "expo-local-authentication": "~13.0.0",
    "expo-av": "~13.2.0",
    "react-native-webview": "13.0.5",
    "ffmpeg-kit-react-native": "^5.1.1",
    "crypto-js": "^4.1.1",
    "zustand": "^4.4.0",
    "react-native-reanimated": "~3.4.0",
    "expo-clipboard": "~4.2.1",
    "react-native-gesture-handler": "~2.9.0",
    "expo-application": "~5.3.0",
    "eventemitter3": "^4.0.7"
  },
  "devDependencies": {
    "@types/react": "~18.0.28",
    "@types/react-native": "~0.72.2",
    "@types/eventemitter3": "^4.0.0",
    "@babel/core": "^7.21.0",
    "@types/react-native-webview": "^11.23.1"
  },
  "expo": {
    "name": "Velox",
    "slug": "velox",
    "scheme": "velox",
    "version": "1.0.0",
    "platforms": [
      "android"
    ],
    "android": {
      "package": "com.velox.app",
      "versionCode": 1,
      "permissions": [
        "FOREGROUND_SERVICE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "READ_MEDIA_AUDIO",
        "READ_MEDIA_VIDEO"
      ]
    },
    "plugins": [
      "./plugins/withFFmpeg"
    ],
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}
